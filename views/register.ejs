  <body>


    <div class="container">
      <h1>Registrate Eon-Core</h1>
      <form role="form" action="/register" method="post" >
        <div class="form-group">
          <input type="text" name="username" placeholder="Enter Username" />
        </div>
        <div class="form-group">
          <input type="password" name="password" placeholder="Password"/>
        </div>
        <button type="submit" class="btn btn-default">Registrate!</button>&nbsp;<a href="/">
          <button type="button" class="btn btn-primary">Cancel</button></a>
      </form>
    </div>

    <footer><a href="http://www.eoncore.mx/" target="_blank"><img src="images/eon_core.png"></a>
      <p>Eon Core <a href="http://www.eoncore.mx/" target="_blank">Silva</a></p>
    </footer>

  </body>
